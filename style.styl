*
	border 1px solid black



.artBlock 
	margin auto
h1
	margin-bottom 10px
	margin-top 0
	width 100%
	color white
	padding-top 50px
	padding-bottom 50px
	text-align center
	
	
.damageControl
	display inline-block
	text-decoration none
	font-size 16px
	color blue
	
.damageControl:hover, .damageControl:active, .damageControl:visited
	text-decoration none
	color blue
	
#row1, #row2, #row3, #row4, #row5 
	margin 25px auto
	
p
	display inline-block
	margin 0
	padding 5px
	
  
	
